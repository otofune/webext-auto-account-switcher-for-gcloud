{
    "name": "Auto Account Switcher for Google Cloud Console",
    "description": "Automatic switch to appropriate account based on authority of the project",
    "version": "0.21",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://console.cloud.google.com/*"
            ],
            "js": ["./src/content.ts"],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        "./src/inject.ts"
    ]
}